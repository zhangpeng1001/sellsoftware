<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.tw.sellsoftware.usercenter.mapper.UserVipRelationMapper">
    <resultMap id="BaseResultMap" type="com.tw.sellsoftware.usercenter.domain.UserVipRelation">
        <id column="id" property="id" jdbcType="INTEGER"/>
        <result column="user_id" property="userId" jdbcType="INTEGER"/>
        <result column="vip_id" property="vipId" jdbcType="INTEGER"/>
    </resultMap>
    <sql id="Base_Column_List">
        id, user_id, vip_id
    </sql>

    <select id="selectByUserId" resultMap="BaseResultMap" parameterType="java.lang.Integer">
        select
        <include refid="Base_Column_List"/>
        from t_user_vip_relation
        where user_id = #{userId,jdbcType=INTEGER}
    </select>

    <insert id="insert" parameterType="com.tw.sellsoftware.usercenter.domain.UserVipRelation" useGeneratedKeys="true" keyProperty="id">
        insert into t_user_vip_relation (user_id, vip_id)
        values (#{userId,jdbcType=INTEGER}, #{vipId,jdbcType=INTEGER})
    </insert>
</mapper>